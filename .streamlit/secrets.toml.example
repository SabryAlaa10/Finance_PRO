# Example secrets file for Streamlit Cloud
# Copy this file to secrets.toml and fill in your values
# NEVER commit secrets.toml to Git!

[database]
# Get this from Neon.tech after creating a database
# Example: postgresql://username:password@host/database?sslmode=require
url = "postgresql://your_username:your_password@your_host.neon.tech/neondb?sslmode=require"

# For local development, you can use:
# url = "postgresql://localhost:5432/finance_app"
